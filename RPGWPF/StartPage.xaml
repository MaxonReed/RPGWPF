      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RPGWPF"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="StartPage">

    <Grid Background="White">
        <Button Content="Start Game" HorizontalAlignment="Left" Margin="300,73,0,0" VerticalAlignment="Top" Width="192" Height="62" FontSize="24" Click="Button0_Click"/>
